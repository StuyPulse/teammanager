#!/usr/bin/env ruby
ENV['GEM_HOME']=ENV['GEM_HOME'] || '/home/stuy/.rvm/gems/ruby-1.9.3-p194'
ENV['GEM_PATH']=ENV['GEM_PATH'] || '/home/stuy/.rvm/gems/ruby-1.9.3-p194:/home/stuy/.rvm/gems/ruby-1.9.3-p194@global'
ENV['PATH']='/home/stuy/.rvm/gems/ruby-1.9.3-p194/bin:/home/stuy/.rvm/gems/ruby-1.9.3-p194@global/bin:/home/stuy/.rvm/rubies/ruby-1.9.3-p194/bin:' + ENV['PATH']


begin
  gem 'rdoc'
rescue NameError => e # --disable-gems
  raise unless e.name == :gem
rescue Gem::LoadError
end

require 'rdoc/ri/driver'

RDoc::RI::Driver.run ARGV
