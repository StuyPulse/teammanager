<% Student::SEASONALS.each do |seasonal_type| %>
  <% if student.valid_seasonal(seasonal_type) %>
    <%= render 'seasonals/valid_button', seasonal_type: seasonal_type, student: student %>
  <% else %>
    <%= render 'seasonals/missing_button', seasonal_type: seasonal_type, student: student %>
  <% end %>
<% end %>
