<tr class="danger" id="invalid-<%= id_name(seasonal_type) %>">
  <td>
    <%= current_year_range %>
    <% if current_user %>
      <%= link_to student_seasonals_path(@student, seasonal: {student_id: @student.id, seasonal_type: seasonal_type}), remote: true, method: :post do %>
        <%= button_tag class: "btn btn-success btn-xs glyphicon glyphicon-ok pull-right" do %>
        <% end %>
      <% end %>
    <% end %>
  </td>
</tr>
